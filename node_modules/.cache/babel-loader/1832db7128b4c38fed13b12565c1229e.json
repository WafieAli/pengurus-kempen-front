,CAAA,IAAA,CAAA,OAAA,CAAA,KAAA;AACA,KAPA,EAQA,UAAA,KAAA,EAAA;AACA,MAAA,KAAA,CAAA,OAAA,GACA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,CAAA,IAAA,IAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,IACA,KAAA,CAAA,OADA,IAEA,KAAA,CAAA,QAAA,EAHA;AAIA,KAbA;AAeA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,gCACA;AAAA;;AACA,WAAA,UAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,OAAA,EAAA;AACA,YAAA,OAAA,EAAA;AACA,cAAA,IAAA,GAAA;AACA,YAAA,kBAAA,EAAA,MAAA,CAAA,OADA;AAEA,YAAA,kBAAA,EAAA,MAAA,CAAA,KAFA;AAGA,YAAA,iBAAA,EAAA,MAAA,CAAA,OAHA;AAIA,YAAA,OAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAJA;AAKA,YAAA,KAAA,EAAA,MAAA,CAAA;AALA,WAAA;AAOA,UAAA,YAAA,CAAA,eAAA,CAAA,IAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,EAAA,IAAA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAHA,EAIA,UAAA,KAAA,EAAA;AACA,YAAA,MAAA,CAAA,OAAA,GACA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA,QAAA,CAAA,IAAA,IAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,IACA,KAAA,CAAA,OADA,IAEA,KAAA,CAAA,QAAA,EAHA;AAIA,WATA;AAWA,UAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA;AACA,OAvBA;AAwBA,KA1BA;AA2BA,IAAA,SA3BA,uBA2BA;AACA,WAAA,UAAA,GAAA,IAAA;AACA;AA7BA;AAlCA,CAAA","sourcesContent":["<template>\n  <div>\n    <div\n      v-if=\"message\"\n      class=\"alert\"\n      :class=\"successful ? 'alert-success' : 'alert-danger'\">\n      <button type=\"button\" class=\"close\" @click=\"message=''\">\n        <span>&times;</span>\n      </button> {{message}}\n    </div>\n    <form name=\"form\" @submit.prevent=\"handleNotification\">\n      <div>\n        <div class=\"form-group\">\n          <b-form-checkbox v-model=\"approve\" name=\"check-button\" switch>\n            Campaign approved/rejected\n          </b-form-checkbox>\n          <b-form-checkbox v-model=\"start\" name=\"check-button\" switch>\n            Campaign start/end\n          </b-form-checkbox>\n          <b-form-checkbox v-model=\"success\" name=\"check-button\" switch>\n            Payment success/fail\n          </b-form-checkbox>\n        </div>\n\n        <div v-if=\"show_email || noti_email\">\n          <div class=\"form-group\">\n            <label for=\"noti_email\">Email</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              name=\"noti_email\"\n              v-model=\"noti_email\"\n              v-validate=\"'email'\"\n            />\n          </div>\n          <div \n            v-if=\"errors.has('noti_email')\"\n            class=\"alert alert-danger\"\n            role=\"alert\">{{errors.first('noti_email')}}\n          </div>\n        </div>\n\n        <br><br>\n\n        <div class=\"form-group\" v-show=\"!noti_email\">\n          <label for=\"addemail\" class=\"desc\">You can also add email address to receive notifications from SmartRetail</label>\n          <b-link name=\"addemail\" @click=\"showEmail\"><center><strong>+Add email</strong></center></b-link>\n        </div> \n\n        <div class=\"form-group\">\n          <button class=\"btn btn-primary btn-block\">Save updates</button>\n        </div>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport {notification} from '../modules/notification.module';\n\nexport default {\n  name: 'NotificationSettings',\n  data() {\n    return {\n      approve: false,\n      start: false,\n      success: false,\n      message: '',\n      noti_email: '',\n      show_email: false\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    notification.getNotiSettings(this.$store.state.auth.user).then(//this is to get the account details from the backend\n      response => {\n        //console.log(response.data.message.[0]);\n        this.approve = response.data.message.campaign_appr_rejc;\n        this.start = response.data.message.campaign_start_end;\n        this.success = response.data.message.payment_succ_fail;\n        this.noti_email = response.data.message.email;\n      },\n      error => {\n        this.content =\n          (error.response && error.response.data && error.response.data.message) ||\n          error.message ||\n          error.toString();\n      }\n    );\n  }, \n  methods: {\n    handleNotification() {\n      this.$validator.validate().then(isValid => {\n        if (isValid) {\n          let data = {\n            campaign_appr_rejc: this.approve,\n            campaign_start_end: this.start,\n            payment_succ_fail: this.success,\n            user_id: this.$store.state.auth.user.id,\n            email: this.noti_email\n          } \n          notification.updNotiSettings(data,this.$store.state.auth.user).then(\n            response => {\n              this.message = response.data.message;\n            },\n            error => {\n              this.message =\n                (error.response && error.response.data && error.response.data.message) ||\n                error.message ||\n                error.toString();\n            }\n          );\n          this.submitted = true;\n          this.successful = true;\n        }\n      });\n    },\n    showEmail(){\n      this.show_email = true;\n    }\n  }\n};\n</script>\n\n<style scoped>\n.desc {\n  display: block;\n  margin-top: 10px;\n  color:#767676;\n  font-size: 14px;\n}\n\n.profile-img-card {\n  width: 96px;\n  height: 96px;\n  margin: 0 auto 10px;\n  display: block;\n  -moz-border-radius: 50%;\n  -webkit-border-radius: 50%;\n  border-radius: 50%;\n}\n</style>\n"],"sourceRoot":"src/views"}]}