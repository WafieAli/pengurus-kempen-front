b-card>\n          <div class=\"col-md-6 col-xl-3\">\n            <b-link v-b-modal.creditcard><b-icon icon=\"pencil\"></b-icon></b-link>\n            <br /><br />\n            <b-icon icon=\"trash\"></b-icon>\n            </div>\n        </div>\n\n        <div class=\"form-group\">\n          <b-link vb-toggle.payment-methods><center><strong>Change payment method</strong></center></b-link>\n        </div>\n\n        <div class=\"form-group\">\n          <button class=\"btn btn-primary btn-block\">Add New Card</button>\n        </div>\n      </div>\n    </form>\n    <div\n    v-if=\"message\"\n    class=\"alert\"\n    :class=\"successful ? 'alert-success' : 'alert-danger'\"\n  >{{message}}</div>\n\n  <!--Modal-->\n  <b-modal id=\"creditcard\" title=\"BootstrapVue\" @ok=\"saveCC\">\n    <div>\n    <div\n      v-if=\"message\"\n      class=\"alert\"\n      :class=\"successful ? 'alert-success' : 'alert-danger'\">\n      <button type=\"button\" class=\"close\" @click=\"message=''\">\n        <span>&times;</span>\n      </button> {{message}}\n    </div>\n    <form name=\"form\">\n      <div>\n        <div class=\"form-group\">\n          <label for=\"bank_name\">Bank Name</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            name=\"bank_name\"\n            v-model=\"bank\"\n          />\n        </div>\n        <div \n          v-if=\"errors.has('bank_name')\"\n          class=\"alert alert-danger\"\n          role=\"alert\">{{errors.first('bank_name')}}\n        </div>\n\n        <div class=\"form-group\">\n          <label for=\"cc_no\">Credit Card Number</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            name=\"cc_no\"\n            v-model=\"card_no\"\n          />\n        </div>\n        <div \n          v-if=\"errors.has('cct_no')\"\n          class=\"alert alert-danger\"\n          role=\"alert\">{{errors.first('cc_no')}}\n        </div>\n      </div>\n    </form>\n  </div>\n  </b-modal>\n  </div>\n</template>\n\n<script>\nimport {biodata} from '../modules/biodata.module';\n\nexport default {\n  name: 'Biodata',\n  data() {\n    return {\n      title: 'Biodata',\n      submitted: false,\n      successful: false,\n      bank: 'HSBC',\n      card_no: '**** **** 783',\n      message: '',\n      creditcard: false\n    };\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    }\n  },\n  mounted() {\n    biodata.getPayment(this.$store.state.auth.user).then(//this is to get the user data from the backend\n      response => {\n        this.firstname = response.data.message.firstname;\n        this.lastname = response.data.message.lastname;\n        this.age = response.data.message.age;\n        this.address = response.data.message.address;\n      },\n      error => {\n        this.content =\n          (error.response && error.response.data && error.response.data.message) ||\n          error.message ||\n          error.toString();\n      }\n    );\n  }, \n  methods: {\n    handleBiodata() {\n      this.$validator.validate().then(isValid => {\n        if (isValid) {\n         let data = {\n           firstname: this.firstname,\n           lastname: this.lastname,\n           age: this.age,\n           address: this.address\n        } \n        biodata.updBiodata(data,this.$store.state.auth.user).then(\n          response => {\n            this.message = response.data.message;\n          },\n          error => {\n            this.message =\n              (error.response && error.response.data && error.response.data.message) ||\n              error.message ||\n              error.toString();\n          }\n        );\n        this.submitted = true;\n        this.successful = true;\n      }\n    });\n   },\n   saveCC: function(){\n     alert(\"ModalOK\");\n   }\n  }\n};\n\n/*function maskify(cc) {\n  return cc.replace(/.(?=.{4})/g, \"#\");\n}*/\n</script>\n\n<style scoped>\nlabel {\n  display: block;\n  margin-top: 10px;\n}\n\n.profile-img-card {\n  width: 96px;\n  height: 96px;\n  margin: 0 auto 10px;\n  display: block;\n  -moz-border-radius: 50%;\n  -webkit-border-radius: 50%;\n  border-radius: 50%;\n}\n</style>\n"]}]}